<nav>
    <ul class="pagination">
        @foreach (var link in Links)
        {
            <li @onclick=@(() => InternalSelectedPage(link))
                style="cursor: pointer"
                class="page-item @(link.Enable ? null : "disabled") @(link.Enable ? "active" : null)">
                <a class="page-link">@link.Text</a>
            </li>
        }
    </ul>
</nav>